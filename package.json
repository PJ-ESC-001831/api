{
  "name": "co-maker-api",
  "version": "1.0.0",
  "description": "The API interface that co-maker uses to operate and integrate with other services.",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:watch": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "build && node dist/index.js",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'"
  },
  "author": "co-maker@tri2b.me",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@clerk/clerk-sdk-node": "^5.0.26",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "express": "^4.19.2",
    "minio": "^8.0.3",
    "mongoose": "^8.5.3",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.14.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-multipart-file-parser": "^0.1.3",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/pg": "^8.11.6",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "drizzle-kit": "^0.24.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
